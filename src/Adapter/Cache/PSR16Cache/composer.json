{
    "name":              "secretary/psr-16-cache-adapter",
    "description":       "PSR-16 Cache Adapter for Secretary",
    "type":              "library",
    "license":           "MIT",
    "keywords":          [
        "secrets",
        "cache",
        "psr-16",
        "secretary"
    ],
    "authors":           [
        {
            "name":  "Aaron Scherer",
            "email": "aequasi@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require":           {
        "php":              "^7.4 || ^8.0",
        "psr/simple-cache": "^1.0 || ^2.0",
        "secretary/core":   "self.version"
    },
    "require-dev":       {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "mockery/mockery": "^1.2"
    },
    "autoload":          {
        "psr-4": {
            "Secretary\\Adapter\\Cache\\PSR16Cache\\": ""
        }
    },
    "autoload-dev":      {
        "psr-4": {
            "Secretary\\Adapter\\Cache\\PSR16Cache\\Tests\\": "tests/"
        }
    }
}
